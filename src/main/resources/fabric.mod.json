{
  "schemaVersion": 1,
  "id": "lottablocks",
  "version": "${version}",

  "name": "Lotta Blocks",
  "description": "A small mod that adds various building blocks for your building palette!",
  "authors": [
    "AmbientUnder",
    "Sydokiddo",
    "LunarLuned",
    "ElBones (Additional Textures)"
  ],
  "contact": {
    "issues": "https://github.com/LunarLuned/lotta_blocks/issues",
    "sources": "https://github.com/LunarLuned/lotta_blocks"
  },
  "license": "MIT",
  "icon": "assets/lottablocks/icon.png",
  "accessWidener": "lottablocks.accesswidener",
  "environment": "*",

  "entrypoints": {
    "main": [
      "net.ambersand.lottablocks.LottaBlocks"
    ],
    "client": [
      "net.ambersand.lottablocks.client.LottaBlocksClient"
    ]
  },
  "mixins": [
    "lottablocks.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.3",
    "fabric-api": "*",
    "minecraft": ">=1.20.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },
  "breaks": {}
}